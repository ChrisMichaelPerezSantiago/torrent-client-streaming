{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "description": "A Vue.js project",
  "author": "Chris Michael <chris.perez@upr.edu>",
  "scripts": {
    "serve": "http-server -o -c 1 -a localhost",
    "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o dist/build.css ] -e src/main.js | uglifyjs -c warnings=false -m > dist/build.js",
    "lint": "eslint --ext .js,.vue src",
    "dev": "npm-run-all --parallel watchify serve",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve watchify -vd -p browserify-hmr -e src/main.js -o dist/build.js",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "watchify": "watchify -vd -p  browserify-hmr -e src/main.js -o dist/build.js"
  },
  "main": "background.js",
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "dependencies": {
    "@vue/cli-service": "^3.6.0",
    "axios": "^0.18.0",
    "buefy": "^0.7.5",
    "drag-drop": "^4.2.0",
    "jquery": "^3.4.1",
    "jszip": "^3.2.1",
    "mediasource": "^2.3.0",
    "moment": "^2.29.2",
    "port": "^0.8.1",
    "prettier-bytes": "^1.0.4",
    "throttleit": "^1.0.0",
    "vue": "^2.0.1",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.0",
    "webtorrent": "^0.103.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.1.0",
    "browserify-css": "^0.14.0",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^2.0.0",
    "electron": "^5.0.0",
    "envify": "^3.4.1",
    "eslint": "^3.3.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "http-server": "^0.9.0",
    "npm-run-all": "^2.3.0",
    "proxyquireify": "^3.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "uglify-js": "^2.5.0",
    "vue-cli-plugin-electron-builder": "^1.3.0",
    "vueify": "^9.0.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "rootDir": "./index.html"
  }
}
